{
  "name": "koa_skeleton",
  "version": "1.0.0",
  "description": "Koa, React and Mongoose",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development && npm run build && node --require babel-polyfill ./dist/server.js",
    "prod": "npm run build && cross-env NODE_ENV=production && node --require babel-polyfill ./dist/server.js",
    "build": "babel server.js config.js -d dist --presets es2015 && babel src -d dist/src --copy-files --presets es2015",
    "buildTest": "babel test -d dist/test --presets es2015",
    "test": "npm run lint && cross-env NODE_ENV=test && npm run base-test",
    "base-test": "./node_modules/.bin/mocha --compilers js:babel-core/register --require babel-polyfill --reporter spec  --recursive ./test/",
    "lint": "eslint src app test webpack config lib server.js webpack.config.js",
    "coverage": "npm run build && npm run buildTest && npm run runCoverage",
    "runCoverage": "./node_modules/.bin/babel-node ./node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register --bail --recursive ./dist/test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redkaras/koa_skeleton.git"
  },
  "keywords": [],
  "author": "redkaras",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.11.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-react": "^6.8.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "nyc": "^10.0.0",
    "supertest": "^2.0.1",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "babel-node-debug": "^2.0.0",
    "bcrypt": "^1.0.2",
    "co": "^4.6.0",
    "co-views": "^2.1.0",
    "koa": "^1.2.4",
    "koa-bodyparser": "^2.3.0",
    "koa-compress": "^1.0.9",
    "koa-error": "^2.1.0",
    "koa-generic-session": "^1.11.4",
    "koa-generic-session-mongo": "^0.3.0",
    "koa-logger": "^1.3.0",
    "koa-passport": "^1.3.0",
    "koa-proxy": "^0.6.0",
    "koa-response-time": "^1.0.2",
    "koa-router": "^5.4.0",
    "koa-sess-mongo-store": "^0.1.1",
    "koa-static-cache": "^3.1.7",
    "lodash": "^4.17.2",
    "mongoose": "^4.7.2",
    "mongoose-class-wrapper": "^0.8.1",
    "mongoose-schema-extend": "^0.2.2",
    "passport-local": "^1.0.0",
    "save": "^2.3.0",
    "swig": "^1.4.2",
    "webpack": "^1.14.0"
  }
}
